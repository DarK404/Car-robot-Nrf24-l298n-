#include <SPI.h>
#include <nRF24L01.h>
#include <RF24.h>

const byte addresses[][6] ={"00001"} ;
RF24 radio(7, 8);
int datos[3];
int val_x;
int val_y;

void setup()
{
  radio.begin();
  radio.openWritingPipe( addresses[0]);
  radio.setRetries(15, 15);
  Serial.begin(9600);
}
void loop() {
  datos[0] = analogRead(1);
  datos[1] = analogRead(0);
  bool ok = radio.write(datos, sizeof(datos));
  Serial.println(datos[0]);
}
